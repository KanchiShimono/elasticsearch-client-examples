{
  "env": {
    "node": true
  },
  "parserOptions": {
    "ecmaVersion": 2019
  },
  "extends": [
    "airbnb-base/legacy",
    "prettier"
  ],
  "globals": {
    "document": true
  },
  "rules": {
    "no-unused-vars": "off"
  },
  "settings": {
    "import/resolver": {
      "node": {
        "extensions": [
          ".js",
          ".jsx",
          ".ts",
          ".tsx"
        ]
      }
    }
  }
}
